<% column_hash.each do |key, value| %>
	Ext.define('Hospital.view.<%= key %>.ViewModel', {
	    extend: 'Ext.app.ViewModel',
	    alias: 'viewmodel.<%= key %>-view',
	    data: {
	        name: 'Hospital',
	    },

	    stores: {
	        <%= key %>Store: 
	        {
	            type: 'store',
	            model: 'Hospital.model.<%= key.capitalize %>',  
	            proxy: {
	                type: 'rest',
	                url : '/<%= key %>',
	                reader: {
	                    type: 'json'
	                },
	                writer: {
	                    type: 'json'
	                }
	            }
	        }
	    }
	});
<% end %>

Ext.define('Hospital.view.basegrid.ViewModel', {
	extend: 'Ext.app.ViewModel',
	alias: 'viewmodel.basegrid-view',
	data: {
	    name: 'Hospital',
	}

});

Ext.define('Hospital.view.main.MainModel', {
    extend: 'Ext.app.ViewModel',

    alias: 'viewmodel.main',

    data: {
        name: 'Hospital'
    }
});
